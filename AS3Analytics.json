{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.6.0",
        "id": "urn:uuid:33045210-3ab8-4636-9b2a-c98d22ab9111",
        "label": "Sample 11",
        "remark": "Simple HTTP application with RR pool",
        "target": {
            "hostname": "bigip1"
        },
        "Test_11_With_Analytics": {
            "class": "Tenant",
            "App1": {
                "class": "Application",
                "template": "http",
                "statsProfile": {
                    "class": "Analytics_Profile",
                    "collectedStatsInternalLogging": true,
                    "collectedStatsExternalLogging": false,
                    "capturedTrafficInternalLogging": false,
                    "capturedTrafficExternalLogging": true,
                    "collectPageLoadTime": true,
                    "collectClientSideStatistics": true,
                    "collectResponseCode": true,
                    "sessionCookieSecurity": "ssl-only"
                },
                "serviceMain": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.0.3.10"
                    ],
                    "pool": "web_pool",
                    "profileAnalytics": {
                        "use": "statsProfile"
                    }
                },
                "web_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "192.0.3.10",
                                "192.0.3.11"
                            ]
                        }
                    ]
                }
            }
        }
    }
}
