{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.7.0",
        "id": "isc-lab",
        "label": "Task3",
        "remark": "Task 3 - HTTPS Application with WAF",
        "target": {
            "hostname": "ip-10-1-1-10.us-west-2.compute.internal"
        },
        "Task3": {
            "class": "Tenant",
            "MyWebApp3": {
                "class": "Application",
                "template": "https",
                "serviceMain": {
                    "class": "Service_HTTPS",
                    "virtualAddresses": [
                        "10.1.20.102"
                    ],
                    "pool": "web_pool",
                    "serverTLS": "webtls",
                    "policyWAF": {
                        "bigip": "/Common/linux-high"
                    }
                },
                "web_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "10.1.10.100",
                                "10.1.10.101"
                            ],
                            "shareNodes": true
                        }
                    ]
                },
                "webtls": {
                    "class": "TLS_Server",
                    "certificates": [
                        {
                            "certificate": "webcert"
                        }
                    ]
                },
                "webcert": {
                    "class": "Certificate",
                    "certificate": {
                        "bigip": "/Common/default.crt"
                    },
                    "privateKey": {
                        "bigip": "/Common/default.key"
                    }
                }
            }
        }
    }
}
